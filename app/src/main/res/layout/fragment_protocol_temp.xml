<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".ui.protocoltemp.ProtocolTempFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/titleProtocolTemp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/displayPadding"
            android:layout_marginTop="@dimen/displayPadding"
            android:fontFamily="@font/clear_sans_bold"
            android:text="@string/protocolList"
            android:textColor="@color/darkGrayText"
            android:textSize="@dimen/titleFont" />

        <View
            android:id="@+id/protocolTempTopLine"
            android:layout_width="match_parent"
            android:layout_height="@dimen/thickLine"
            android:layout_marginTop="@dimen/lineMarginTitle"
            android:background="@color/line" />
    </LinearLayout>

</layout>